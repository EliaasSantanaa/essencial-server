{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "*": false
    }
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "functions": {
    "dist/main.js": {
      "includeFiles": "node_modules/swagger-ui-dist/**"
    }
  }
}