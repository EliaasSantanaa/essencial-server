{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "*": false
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "entrypoint": "dist/main.js",
        "includeFiles": [
          "node_modules/swagger-ui-dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "package.json"
    }
  ]
}